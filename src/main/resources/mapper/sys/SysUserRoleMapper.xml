<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.cy.pj.sys.dao.SysUserRoleDao">
   <insert id="insertObjects">
    insert into sys_user_roles (user_id,role_id) 
    values 
    <foreach collection="roleIds" open="(" separator="," close=")" item="roleId">
      #{userId},#{roleId}
    </foreach>
   </insert>
   <select id="findRoleIdsByUserId"  resultType="int">
   select  role_id from sys_user_roles where user_id= #{id}
   </select>
    <delete id="deleteObjectsByUserId"
           parameterType="int">
   
         delete from sys_user_roles
         where user_id=#{userId}
                 
   </delete>
    
  </mapper>